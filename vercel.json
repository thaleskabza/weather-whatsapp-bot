{
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/api/:path*"
      },
      {
        "source": "/swagger",
        "destination": "/swagger/index.html"
      },
      {
        "source": "/swagger/:path*",
        "destination": "/swagger/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }